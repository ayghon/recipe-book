{
  "expo": {
    "experiments": {
      "tsconfigPaths": true
    },
    "scheme": "recipe-book",
    "name": "Recipe Book",
    "slug": "recipe-book",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#009688"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ayghon.recipeBook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#009688"
      },
      "package": "com.ayghon.recipeBook"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-localization"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "95e297d3-95ce-4f23-9969-ce2a77125ef6"
      }
    }
  }
}
